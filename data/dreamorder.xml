<?xml version="1.0" encoding="UTF-8"?>
<!--
Dream Order will have a burger for Dima, and takeout
-->

<!DOCTYPE order SYSTEM 'bonanza.dtd'>

<order type="takeout">
    <customer>Dima</customer>
    <burger>
        <patty type="beef"/>
